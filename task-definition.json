{
    "family": "boardtask",
    "networkMode": null,
    "containerDefinitions": [
      {
        "name": "board_web",
        "image": "760055059860.dkr.ecr.ap-northeast-1.amazonaws.com/board",
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
          }
        ],
        "environment": [
         {
           "name": "BOARD_DB_HOST",
           "value": "boarddatabase.cchpc7kznfed.ap-northeast-1.rds.amazonaws.com"
         },
         {
           "name": "BOARD_DB_NAME",
           "value": "boarddata"
         },
         {
           "name": "BOARD_DB_PASSWORD",
           "value": "password"
         },
         {
           "name": "BOARD_DB_USER",
           "value": "root"
         },
         {
           "name": "BOARD_REDIS_HOST",
           "value": "boardredis.67kw0i.clustercfg.apne1.cache.amazonaws.com"
         },
         {
           "name": "BOARD_REDIS_PORT",
           "value": "6379"
         }
        ],
        "essential": true
      }
    ],
    "requiresCompatibilities": [
      "EC2"
    ],
    "executionRoleArn": "ecsTaskExecutionRole",
    "cpu": "256",
    "memory": "512"
}
