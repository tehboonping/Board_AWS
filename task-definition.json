{
    "family": "boardtask",
    "networkMode": null,
    "containerDefinitions": [
      {
        "name": "board_web",
        "image": "board",
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
          }
        ],
        "essential": true
      }
    ],
    "requiresCompatibilities": [
      "EC2"
    ],
    "executionRoleArn": "ecsTaskExecutionRole",
    "cpu": "256",
    "memory": "512"
}
